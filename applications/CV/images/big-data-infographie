                                            return '<div class="image"><img src="' + obj.mediaPath.replace("{{options}}", "w_83,q_60,") + '" /></div><div class="title"><p>' + obj._highlightResult.tradingName.value + '</p><p class="offers">' + offerTitle('Offre', 'Offres', obj.totalOffers) + '</p></div><a href="#" class="login-button action-favourites btn-favorite selected" data-endpoint="/account/updatefavouritemerchant" data-merchant="' + obj.objectID + '" data-favourite-link="searchedmerchant" data-add="false"><span class="added">Ajout&#233; aux favoris</span><span class="add">Ajouter aux favoris</span></a>';
                                        } else {
                                            return '<div class="image"><img src="' + obj.mediaPath.replace("{{options}}", "w_83,q_60,") + '" /></div><div class="title"><p>' + obj._highlightResult.tradingName.value + '</p><p class="offers">' + offerTitle('Offre', 'Offres', obj.totalOffers) + '</p></div><a href="#" class="login-button action-favourites btn-favorite" data-endpoint="/account/updatefavouritemerchant" data-merchant="' + obj.objectID + '" data-favourite-link="searchedmerchant" data-add="true"><span class="added">Ajout&#233; aux favoris</span><span class="add">Ajouter aux favoris</span></a>';
                                        }
                                    } else if (isSimple) {
                                        return '<div class="title"><p>' + obj._highlightResult.tradingName.value + '</p></div>';
                                    }
                                    else {
                                        return '<a href="#"><div class="image"><img src="' + obj.mediaPath.replace("{{options}}", "w_83,q_60,") + '" /></div><div class="title"><p>' + obj._highlightResult.tradingName.value + '</p><p class="offers">' + offerTitle('Offre', 'Offres', obj.totalOffers) + '</p></div></a>';
                                    }
                                }
                            }
                        }).on('typeahead:selected', function (e, data) {

                            _gaq.push(['_trackPageview', generateTrackingLink(data.tradingName)]);

                            var form = $this.parents('form').first(),
                                isFavouriteSearch = $this.data("favourites") == true;

                            if(!isFavouriteSearch && !isSimple){
                                form.submit();
                            }

                        }).keypress(function(e){
                            if(isFavouriteSearch){
                                if (e.which == 13){
                                    return false;
                                }
                            }
                        });
                    });
                }

                attachTypeAhead(searchBrand);
                attachTypeAhead(searchBrand2);

            })();</script></body></html>